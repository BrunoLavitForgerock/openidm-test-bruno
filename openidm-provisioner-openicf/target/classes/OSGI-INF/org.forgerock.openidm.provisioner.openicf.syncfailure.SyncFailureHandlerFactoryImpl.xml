            <provide interface="org.forgerock.openidm.provisioner.openicf.syncfailure.SyncFailureHandlerFactory"/>
        </service>
        <property name="service.pid" value="org.forgerock.openidm.openicf.syncfailure"/>
        <reference name="scriptRegistry" interface="org.forgerock.script.ScriptRegistry" cardinality="1..1" policy="dynamic" bind="bindScriptRegistry" unbind="unbindScriptRegistry"/>
